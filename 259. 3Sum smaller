class Solution:
    def threeSumSmaller(self, nums: List[int], target: int) -> int:
      nums.sort()
      count  = 0
      n = len(nums)
      
      for i in range(n):
        low = i + 1
        high  = n - 1
        
        while low < high:
          sum  = nums[i] + nums[low] + nums[high]
          if sum < target:
            count += high - low
            low += 1
          else:
            high -= 1
            
         return count
