class Solution:
    def findKthLargest(self, nums: List[int], k: int) -> int:
        return heapq.nlargest(k,  nums)[-1]
        
        
class Solution:
    def findKthLargest(self, nums: List[int], k: int) -> int:
        nums_new = nums.sort(reverse = True)
        return nums[k - 1]
        
#两种方法
